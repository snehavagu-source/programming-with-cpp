 program:
 #include<iostream>
 #include<map>
 usingnamespacestd;
 intmain()
 {
 map<int,string>students;
 cout<<”Rollno:24B11AI438”<<endl;
 cout<<"===MAPOPERATIONS==="<<endl;
 students[101]="Alice";
 students[102]="Bob";
 students[103]="Charlie";
 students.insert({104,"David"});
 cout<<"Studentsafterinsertion:"<<endl;
 for(autox:students)
 cout<<"RollNo:"<<x.first<<"Name:"<<x.second<<endl;
 cout<<"\nAccesselementwithkey102:"<<students[102]<<endl;
 intkey=103;
 autoit=students.find(key);
 if(it!=students.end())
 cout<<"FoundstudentwithRollNo"<<key<<":"<<it->second<<endl;
 else
 cout<<"StudentwithRollNo"<<key<<"notfound!"<<endl;
 students.erase(101);
 cout<<"\nAfterdeletingkey101:"<<endl;
 for(autox:students)
 cout<<"RollNo:"<<x.first<<"Name:"<<x.second<<endl;
 cout<<"\nTotalstudents:"<<students.size()<<endl;
 return0;
 }
